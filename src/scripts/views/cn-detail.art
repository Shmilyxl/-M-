<div class='cin-container'>

    <header>{{res.cinemaData.nm}}</header>
    <a id="download-header" data-cid="c_dmLad" data-bid="b_53up23as" data-lab="{postion:'home', utm_content: , utm_source: }">
        <div class="download-header" data-download='advertise'>
            <div class="logo-img"></div>
            <div class="info-wrapper">
                <div class="title">猫眼</div>
                <div class="content">0 元看电影，天天领现金</div>
            </div>
            <div class="link-btn">立即领取<img src="http://p0.meituan.net/moviemachine/89857785f50685578fa61e1883251da95640.png"></img>
            </div>
        </div>
    </a>

    <div class="body_wrap">

        <div class="cinema_Wrap">
            <div class="cin_info">
                <div class="title">{{res.cinemaData.nm}}</div>
                <div class="location">{{res.cinemaData.addr}}</div>
            </div>
            <div class="location_icon">
                <img src="data:image/png;base64,"
                    alt="">
            </div>
        </div>
        <div class="cn_nav">
            <div>
                <div class="cinema-nav">
                    <div class="post-bg"></div>
                    <div class="post-bg-f"></div>
                    <div class="swiper-container">
                        <div class="swiper-wrapper">
                            {{each res.showData.movies}}
                            <div class="swiper-slide" style="width:80px ;"><img data-id='{{$value.id}}' style="width: 65px;height: 95px;" src="{{$value.img.replace(/\/w.h\//,/128.180/)}}" alt=""></div>
                            {{/each}}
                        </div>
                    </div>

                </div>
                <div class="movie_info">



                </div>

            </div>
            <div class="liwrap"></div>

            <div class="tuan-wrap">
                <div class="gap">
                </div>
                <div class="tuan-list">
                    <div class="tuan-title">
                        影院超值套餐
                    </div>
                    {{each res.dealList.dealList}}
                    <div class="tuan-list-item">
                        <img src="//p0.meituan.net/440.0/movie/4b94cdb55d271185e2ecb17a97a6f61f1020393.png@750w_750h_1e_1c" alt="">
                        <span class="hot-tag">Hot</span>
                        <div class="item-info">
                            <div class="title">
                                {{$value.title}}
                            </div>
                            <div class="sell-num">321售</div>
                            <div class="price">￥38
                            </div>
                            <div class="buy-btn">去购买</div>
                        </div>
                    </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</div>