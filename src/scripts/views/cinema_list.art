{{each list}}
<div class='cineme_item_wrap' data-id="{{$value.id}}">
    <li class='cineme_item'>
        <div class='cineme_title'><span class="cineme_nm">{{$value.nm}}</span><span class="cineme_price"><span class="price">{{$value.sellPrice}}</span>元起</span>
        </div>
        <div class='local_distance'>
            <div class='local'>{{$value.addr}}</div>
            <div class='distance'>{{$value.distance}}</div>
        </div>
        <div class="lable_block">
            <div class='allowrefound'>退</div>
            <div class='endorse'>小吃</div>
            <div class='snack'>改签</div>
            <div class='vipTag'>折扣卡</div>
        </div>
        <div class='discount_block'>
            <div class='discount_card'>
                <img src="data:image/png;base64,">
            </div>
            <div class='discount_text'>{{$value.promotion.cardPromotionTag}}</div>
    </li>
    </div>
    {{/each}}